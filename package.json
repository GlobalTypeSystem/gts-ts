{
  "name": "@globaltypesystem/gts-ts",
  "version": "0.2.0",
  "description": "TypeScript library for working with GTS (Global Type System) identifiers and JSON/JSON Schema artifacts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gts": "./dist/cli/index.js",
    "gts-server": "./dist/server/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "server": "node dist/server/index.js",
    "server:dev": "ts-node src/server/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GlobalTypeSystem/gts-ts.git"
  },
  "keywords": [
    "gts",
    "global-type-system",
    "json",
    "jsonschema",
    "validation"
  ],
  "author": "Global Type System",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "ajv": "^8.18.0",
    "ajv-formats": "^2.1.1",
    "commander": "^12.0.0",
    "fastify": "^5.7.3",
    "uuid": "^9.0.1"
  }
}
